{
  "json.schemas": [
    {
      "fileMatch": [
        "/src/modules/changes/data/*.json"
      ],
      "url": "./src/modules/changes/changesSchema.json"
    },
    {
      "fileMatch": [
        "/src/modules/download/download.json"
      ],
      "url": "./src/modules/download/downloadSchema.json"
    }
  ],
  "typescript.format.semicolons": "insert",
  "javascript.format.semicolons": "insert",
  "typescript.tsserver.experimental.enableProjectDiagnostics": true
}